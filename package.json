{
  "name": "cerberus-website",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "gulp": "gulp",
    "concurrently": "concurrently",
    "build:icons": "gulp build:icons",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:client-bundle": "ng build --prod",
    "build:server-bundle": "ng build --prod --app 1 --output-hashing=false",
    "build:server": "gulp build:server",
    "build": "yarn build:client-bundle && yarn build:server-bundle && yarn build:server",
    "start": "node dist/backend/server.js",
    "watch:client-bundle": "ng build --delete-output-path=false --watch",
    "watch:server-bundle": "ng build --delete-output-path=false --aot --app 1 --output-hashing=false --watch",
    "watch:ng": "concurrently --kill-others 'yarn watch:client-bundle' 'yarn watch:server-bundle'",
    "watch:server": "gulp watch:server",
    "watch": "concurrently --kill-others 'yarn watch:ng' 'yarn watch:server'",
    "serve": "concurrently --kill-others 'yarn watch' 'yarn gulp serve'",
    "lint:styles": "gulp lint:styles"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.8",
    "@angular/cdk": "^5.2.4",
    "@angular/common": "^5.2.8",
    "@angular/compiler": "^5.2.8",
    "@angular/core": "^5.2.8",
    "@angular/forms": "^5.2.8",
    "@angular/http": "^5.2.8",
    "@angular/material": "^5.2.4",
    "@angular/platform-browser": "^5.2.8",
    "@angular/platform-browser-dynamic": "^5.2.8",
    "@angular/platform-server": "^5.2.8",
    "@angular/router": "^5.2.8",
    "@nguniversal/express-engine": "^5.0.0-beta.6",
    "@nguniversal/module-map-ngfactory-loader": "^5.0.0-beta.6",
    "core-js": "^2.5.3",
    "express": "^4.16.2",
    "express-mailer": "^0.3.1",
    "gulp": "^3.9.1",
    "gulp-font-icons": "^0.1.39-beta",
    "gulp-sass-lint": "^1.3.4",
    "nodemailer": "^4.6.2",
    "rxjs": "^5.5.6",
    "ts-loader": "^4.0.1",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "1.7.3",
    "@angular/compiler-cli": "^5.2.8",
    "@angular/language-service": "^5.2.8",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~9.4.7",
    "codelyzer": "^4.2.1",
    "concurrently": "^3.5.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-typescript": "^4.0.1",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.4.2",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
